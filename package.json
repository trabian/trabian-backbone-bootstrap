      "underscore:underscore.js",
      "backbone:backbone.js",
      "vendor/backbone.modelbinding.js",
      "vendor/backbone.ModelBinder.js",
      "vendor/backbone.validation.js",
      "lib/backbone-extensions.coffee",
      "vendor/backbone.localStorage.js",
      "vendor/backbone-relational.js"
