{
  "name": "trabian-backbone-bootstrap",
  "version": "0.0.19",
  "description": "Shared views and styles using Backbone and Twitter Bootstrap",
  "publishConfig": {
    "registry": "http://npm.trabian.iriscouch.com"
  },
  "main": "./lib/index.coffee",
  "dependencies": {
    "backbone": "0.9.x",
    "coffee-script": "1.2.x",
    "express": "2.5.x",
    "jade": "0.20.x",
    "jquery-browser": "1.7.x",
    "underscore": "1.3.x",
    "stitch-up":"~ 0.3.3"
  },
  "stitch": {
    "paths": [
      "src"
    ],
    "images": ["images"],
    "dependencies": [
      "underscore:underscore.js",
      "backbone:backbone.js",
      "vendor/backbone.modelbinding.js",
      "vendor/backbone.validation.js",
      "lib/backbone-extensions.coffee",
      "vendor/backbone.localStorage.js",
      "vendor/backbone-relational.js"
    ],
    "vendorDependencies": [
      "jquery-browser:lib/jquery.js",
      "vendor/bootstrap.js"
    ]
  }
}
