{
  "name": "trabian-backbone-bootstrap",
  "version": "0.0.30",
  "description": "Shared views and styles using Backbone and Twitter Bootstrap",
  "publishConfig": {
    "registry": "http://npm.trabian.iriscouch.com"
  },
  "main": "./lib/index.coffee",
  "dependencies": {
    "backbone": "0.9.2",
    "coffee-script": "1.2.x",
    "express": "2.5.x",
    "jade": "0.20.x",
    "jquery-browser": "1.7.2-3",
    "underscore": "1.8.3",
    "stitch-up": "0.3.12"
  },
  "stitch": {
    "paths": ["src"],
    "images": ["images"],
